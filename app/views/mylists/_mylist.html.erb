    <% if @dress.mylisted_by?(current_user) %>
    <%= link_to "マイリスト追加", dress_mylists_path(@dress.id), data: { turbo_method: :delete }, class: "dress_mylists" , remote: true %>
    <% else %>
    <%= link_to "マイリスト削除", dress_mylists_path(@dress.id), data: { turbo_method: :post }, class: "dress_mylists" , remote: true %>
    <% end %>